{
	"manifest_version": 2,
	"name": "tab tracker",
	"description": "tracks time spent on opened websites",
	"version": "0.1",
	"author": "Shaphat",
	"browser_action": {
		
		"default_title": "Just observing your current url."
	},
	"permissions": ["tabs", "activeTab", "https://web-tracker-hhjh.onrender.com/*", "storage"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
}